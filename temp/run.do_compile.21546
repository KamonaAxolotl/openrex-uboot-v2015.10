	fi

	unset LDFLAGS
	unset CFLAGS
	unset CPPFLAGS

	if [ ! -e /home/hardware2/fsl-community-bsp/build/tmp/work/imx6qsabresd-poky-linux-gnueabi/u-boot-fslc/v2015.10+gitAUTOINC+1b6aee73e6-r0/git/.scmversion -a ! -e /home/hardware2/fsl-community-bsp/build/tmp/work/imx6qsabresd-poky-linux-gnueabi/u-boot-fslc/v2015.10+gitAUTOINC+1b6aee73e6-r0/git/.scmversion ]
	then
